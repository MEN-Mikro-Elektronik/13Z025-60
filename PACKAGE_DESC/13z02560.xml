			<setting>
				<name>RX_BUFF_SIZE</name>
				<description>driver internal Rx buffer size</description>
				<type>U_INT32</type>
				<defaultvalue>512</defaultvalue>
				<choises>
					<choise>
						<value>128</value>
						<description>byte</description>
					</choise>
					<choise>
